.banner{
  overflow: hidden;
  height: 380rpx;
}
.banner image{
  width: 100%;
  height: 100%;
}
.grids{
  display: flex;
  flex-wrap: wrap;
  border-left: 1rpx solid #ccc
}
.grids .items{
  display: flex;
  width: 33.3%;
  height: 250rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-top: 1rpx solid #ccc;
  border-right: 1rpx solid #ccc; 
  box-sizing: border-box;

}
.grids .items image
{
  width: 70rpx;
  height: 70rpx;
} 
